{
    "title": "address",
    "description": "Model for searching by address in AST",
    "type": "object",
    "properties": {
      "Line_1": {
        "type": "string"
      },
      "Line_2": {
        "type": "string"
      },
      "City": {
        "type": "string",
        "pattern": "^[A-Za-z\\s\\-\\.]*$"
      },
      "State": {
        "$ref": "state.json"
      },
      "Zip": {
        "type": "string",
        "pattern": "^\\d{5}$"
      },
      "Zip_4": {
        "type": "string",
        "pattern": "^\\d{4}$"
      }
    },
    "required": [
        "Line_1",
        "City",
        "State",
        "Zip"
    ]
  }