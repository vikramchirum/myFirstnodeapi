{
    "title": "service_location",
    "description": "Model for returing address search results in AST",
    "type": "object",
    "properties": {
      "Line_1": {
        "type": "string"
      },
      "Line_2": {
        "type": "string"
      },
      "City": {
        "type": "string",
        "pattern": "^[A-Za-z\\s\\-\\.]*$"
      },
      "State": {
        "$ref": "state.json"
      },
      "Zip": {
        "type": "string",
        "pattern": "^\\d{5}$"
      },
      "Zip_4": {
        "type": "string",
        "pattern": "^\\d{4}$"
      },
      "Esiid": {
        "type": "string"
      },
      "Premise_Type": {
        "type": "string"
      },
      "Status": {
        "type": "string"
      },
      "Meter_Type": {
        "type": "string"
      },
      "Switch_Hold": {
        "type": "boolean"
      },
      "Debt_Hold": {
        "type": "boolean"
      }
    }
  }