{
  "get": {
    "tags": [
      "auth"
    ],
    "summary": "Gets new access token using a refresh token",
    "responses": {
      "400": {
        "description": "Authentication failed",
        "content": {
          "text/plain": {
            "schema": {
              "type": "string"
            },
            "example": "Invalid refresh token"
          }
        }
      },
      "200": {
        "description": "Ok",
        "content": {
          "application/json": {
            "schema": {
              "type": "string"
            },
            "example": "xxxxxx.yyyyyyy.zzzzzz"
          }
        }
      }
    }
  }
}
